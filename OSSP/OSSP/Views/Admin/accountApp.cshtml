@model List<OSSP.Models.users>
@{
    Layout = "_Header";
}
<div id="navbar"></div>

<!-- Begin Page Content -->
<div class="container-fluid">

    <!-- Page Heading -->
    <h1 class="h3 mb-2 text-gray-800">Account(Application)</h1>

    <!-- DataTales Example -->
    <div class="card shadow mb-4">

        <div class="card-body">
            <div class="table-responsive">
                <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                    <thead>
                        <tr>
                            <th>Details</th>
                            <th>Date Applied</th>
                            <th>Resident No.</th>
                            <th>Last Name</th>
                            <th>First Name</th>
                            <th>Email</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>
                                    <a class="btn btn-success" href="account?resNo=@item.resident_account"><i class="fa-solid fa-eye"></i> View</a>
                                </td>
                                <td>@item.dateString</td>
                                <td>@item.resident_account</td>
                                <td>@item.last_name</td>
                                <td>@item.first_name</td>
                                <td>@item.email_address</td>


                            </tr>
                        }

                    </tbody>
                </table>
            </div>
        </div>
    </div>

</div>
<script>

    $(document).ready(function () {
        $("#dataTable").DataTable();
    });
</script>